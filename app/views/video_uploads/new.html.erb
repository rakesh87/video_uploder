<div class="container-fluid mt-60">
  <h1>Upload</h1>
 
  <%= form_for @video do |f| %>
    <%= render 'shared/errors', object: @video %>
 
    <div class="form-group">
		  <%= f.label :file %>
		  <%= f.file_field :file, class: 'form-control', required: true %>
		</div>

    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>